package grupoD.associapp.modelo;

public class Aptidao {
	private int id;
	private String nome;
	private Comunidade comunidadePertencente;
	
	public Aptidao(String nome){
		this.nome=nome;
	}
	public Aptidao(){};
	
	@Override
	public String toString() {
		return "Contato ("+ nome +"): ";
	}
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
	public String getComunidade() {
		// TODO Auto-generated method stub
		return null;
	}
	public void setComunidade(Comunidade comunidadePertencente) {
		this.comunidadePertencente = comunidadePertencente;
	}
}
