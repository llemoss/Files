package grupoD.associapp.modelo;

public class Parentesco {
	
	private int id;
	private Pessoa parente;
	private TipoParentesco tipo;
	
	public Parentesco(){}
	
	public Parentesco(Pessoa pessoa, TipoParentesco tipo){
		this.parente = pessoa;
		this.tipo = tipo;
	}
	
	public Pessoa getParente() {
		return parente;
	}
	
	public void setParente(Pessoa pessoa) {
		this.parente = pessoa;
	}
	
	public TipoParentesco getTipo() {
		return tipo;
	}
	
	public void setTipo(TipoParentesco tipo) {
		this.tipo = tipo;
	}

	public void setId(int id) {
		// TODO Auto-generated method stub
		
	}
	
}
