	private boolean[] operand2;

	private Boolean branchTaken;

	public boolean[] execute(OperationCode opCode) {
		return null;
	}

	public boolean[] getOperand1() {
		return operand1;
	}

	public void setOperand1(boolean[] operand1) {
		this.operand1 = operand1;
