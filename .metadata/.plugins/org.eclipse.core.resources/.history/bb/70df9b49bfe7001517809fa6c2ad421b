public class ALU {
	
	private short operand1;
	private short operand2;
	private boolean branchTaken = false;
	
	private short Execute(OperEnum op){
		return (Short) null;
	}

	public short getOp1() {
		return operand1;
	}

	public void setOp1(short operand1) {
		this.operand1 = operand1;
	}

	public short getOp2() {
		return operand2;
	}

	public void setOp2(short operand2) {
		this.operand2 = operand2;
