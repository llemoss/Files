package grupoD.associapp.interfaces;

import java.util.List;

import grupoD.associapp.modelo.Recurso;

public interface RecursoDAO {
	public void createTable();
	public List<Recurso> lista();
	public Recurso inserir(Recurso p);
	public void remover(Recurso p);
	public Recurso obter(int id);
	List<Recurso> obterRecursos(int id_comunidade);
	List<Recurso> obterRecursos(int id_familia);
}
